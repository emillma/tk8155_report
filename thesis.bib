  title = {Image Demosaicing: A Systematic Survey},
  shorttitle = {Image Demosaicing},
  booktitle = {Visual {{Communications}} and {{Image Processing}} 2008},
  author = {Li, Xin and Gunturk, Bahadir and Zhang, Lei},
